<div class="col-md-6 offset-md-3">
    <h1>
        Users
    </h1>
    <div>
        <div class="form-group">
                @foreach (var usr in ViewData["username"] as List<Tuple<int, string>>)
                {
                    <a href="@Url.Action("Index", "Usuarios", new {uid = usr.Item1})" class="btn-link" id="@usr.Item1">@usr.Item2</a><br><br>
                }
        </div>

    </div>
</div>
