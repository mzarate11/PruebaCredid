<div class="col-md-10 offset-md-3">
    <div class="row">
        <div class="col-md-6">
            <br>
            <h3>@ViewData["user"]</h3>
            <h3>@ViewData["titulo"]</h3>
            <br>
            <h5>@ViewData["posteo"]</h5>
        </div>

        <div>
            <br>
            <br>
            <h4>Comments</h4>
            <br>
            @foreach (var com in ViewData["Comments"] as List<Tuple<string, string, string>>)
            {
                
                <div class="col-md-6">
                    <h6>@com.Item2</h6>
                    <label>@com.Item1</label>
                    <br>
                    <label>@com.Item3</label>
                    
                </div>
                <br>
            }
        </div>
     </div>

</div>